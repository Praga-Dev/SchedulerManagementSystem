@model Response<List<AccessLevelTypeInfo>>

<div class="form-group">
    <label class="form-label">
        BPA Level Type
        <span class="mandatory">*</span>
    </label>
    <select id="selectBPALevelType" name="selectBPALevelType" class="form-control form-control-sm">
        @if (Model != null && Model.Data != null && Model.Data.Any())
        {
            <option disabled selected value=""> -- select an option -- </option>
            @foreach (var _BPALevelType in Model.Data)
            {
                <option value="@_BPALevelType.Id">@_BPALevelType.Name</option>
            }
        }
    </select>
</div>