@model Response<List<AccessLevelTypeInfo>>

<div class="action-body-container">
    <div class="main-grid-container">
        <div class="pk-table-wrapper">
            <table class="table table-bordered align-middle table-hover table-responsive">
                <thead>
                    <tr>
                        <th class="header" scope="col">
                            <p class="h6 fw-bold">AccessLevelType Name</p>
                        </th>
                        <th class="header" scope="col">
                            <div class="d-block">
                                <p class="h6 fw-bold text-right">Actions</p>
                            </div>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null && Model.IsSuccess && Model.Data != null && Model.Data.Any())
                    {
                        @foreach (var accessLevelType in Model.Data)
                        {
                            <tr>
                                <td scope="row">
                                    @if (!string.IsNullOrWhiteSpace(accessLevelType.Name))
                                    {
                                        <p>@accessLevelType.Name</p>
                                    }
                                </td>
                                <td scope="row">
                                    <div class="d-flex justify-content-end">
                                        <span class="material-icons-outlined arrow btn-icon btnEditAccessLevelType" id="btnEditAccessLevelType_@accessLevelType.Id"
                                              title="Update AccessLevelType" onclick="editAccessLevelType('@accessLevelType.Id')">
                                            edit
                                        </span>
                                        <span class="material-icons-outlined arrow btn-icon btnDeleteAccessLevelType" id="btnDeleteAccessLevelType_@accessLevelType.Id"
                                              title="Delete AccessLevelType" onclick="deleteAccessLevelType('@accessLevelType.Id', '@accessLevelType.Name')">
                                            delete
                                        </span>
                                    </div>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>