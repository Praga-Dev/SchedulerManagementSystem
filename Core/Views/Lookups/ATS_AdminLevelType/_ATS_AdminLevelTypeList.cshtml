@model Response<List<ATS_AdminLevelTypeInfo>>

<div class="action-body-container">
    <div class="main-grid-container">
        <div class="pk-table-wrapper">
            <table class="table table-bordered align-middle table-hover table-responsive">
                <thead>
                    <tr>
                        <th class="header" scope="col">
                            <p class="h6 fw-bold">ATS AdminLevelType Name</p>
                        </th>
                        <th class="header" scope="col">
                            <div class="d-block">
                                <p class="h6 fw-bold text-right">Actions</p>
                            </div>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null && Model.IsSuccess && Model.Data != null && Model.Data.Any())
                    {
                        @foreach (var aTS_AdminLevelType in Model.Data)
                        {
                            <tr>
                                <td scope="row">
                                    @if (!string.IsNullOrWhiteSpace(aTS_AdminLevelType.Name))
                                    {
                                        <p>@aTS_AdminLevelType.Name</p>
                                    }
                                </td>
                                <td scope="row">
                                    <div class="d-flex justify-content-end">
                                        <span class="material-icons-outlined arrow btn-icon btnEditATS_AdminLevelType" id="btnEditATS_AdminLevelType_@aTS_AdminLevelType.Id"
                                              title="Update ATS AdminLevelType" onclick="editATS_AdminLevelType('@aTS_AdminLevelType.Id')">
                                            edit
                                        </span>
                                        <span class="material-icons-outlined arrow btn-icon btnDeleteATS_AdminLevelType" id="btnDeleteATS_AdminLevelType_@aTS_AdminLevelType.Id"
                                              title="Delete ATS AdminLevelType" onclick="deleteATS_AdminLevelType('@aTS_AdminLevelType.Id', '@aTS_AdminLevelType.Name')">
                                            delete
                                        </span>
                                    </div>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>