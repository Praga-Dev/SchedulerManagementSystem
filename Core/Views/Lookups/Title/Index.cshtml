@model Response<List<TitleInfo>>

@{
    ViewData["Title"] = "Title";
}

<div class="pk-table-container">
    <div class="table-title">
        <div class="row">
            <div class="col-sm-6">
                <h2>Title</h2>
            </div>
            <div class="col-sm-6">
                <!-- Button trigger modal -->
                <button type="button" class="btn btn-primary-outline" onclick="onCreateTitle()">
                    <span>Create &nbsp;</span><i class="material-icons">&#xE147;</i>
                </button>
            </div>
        </div>
    </div>
    <div id="titleListContainer">
        @if (Model != null && Model.IsSuccess && Model.Data != null && Model.Data.Any())
        {
            <partial name="_TitleList.cshtml" />
        }
        else
        {
            <p>
                NO RECORDS FOUND
            </p>
        }
    </div>
</div>
<div id="createTitleFormContainer">
    <partial name="_CreateTitle.cshtml" model="new TitleInfo()" />
</div>

<script src="~/js/lookups/title/title.js" asp-append-version="true"></script>